<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="MainMenuMemberController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label fx:id="title" layoutX="100" layoutY="50" text="Main Menu"/>
        <Button text="View Profile" layoutX="100" layoutY="100" onAction="#handleProfile"/>
        <Button text="Log a Workout" layoutX="100" layoutY="150" onAction="#handleLog"/>
        <Button text="Workout Statistics" layoutX="100" layoutY="200" onAction="#handleStats"/>
        <Button text="Workout History" layoutX="100" layoutY="250" onAction="#handleHistory"/>
        <Button text="Join or view a challenge" layoutX="100" layoutY="300" onAction="#handleChallenge"/>
        <Button text="Log Out" layoutX="100" layoutY="350" onAction="#handleLogOut"/>
    </children>
</AnchorPane>


