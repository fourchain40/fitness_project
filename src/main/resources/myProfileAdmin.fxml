<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="AdminProfileController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label fx:id="title" layoutX="100" layoutY="50" text="My Profile"/>
        <Label fx:id="name" layoutX="100" layoutY="100"/>
        <Button text="Return to Menu" layoutX="100" layoutY="250" onAction="#handleBack"/>
    </children>

</AnchorPane>
